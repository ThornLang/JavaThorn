// Test that regular methods still work with local variables
class RegularMethodTest {
    $ init() {
        name: string = "TestClass";
    }
    
    $ process_data(input: string) {
        // These should be local variables, not properties
        temp_value: string = input + " processed";
        counter: number = 0;
        
        // Local variable usage with for loop
        for (i = 0; i < 3; i = i + 1) {
            counter = counter + 1;
        }
        
        return temp_value + " (count: " + counter + ")";
    }
    
    $ get_name() {
        return this.name;
    }
}

test = RegularMethodTest();
print("Name: " + test.get_name());
print("Result: " + test.process_data("data"));