// Test the test module assertions
import { assertTrue, assertFalse, assertEqual, assertNotEqual, assertNull, assertNotNull } from "test";

// Test assertTrue
assertTrue(true, "true should be true");
assertTrue(1 == 1, "1 should equal 1");
assertTrue(5 > 3, "5 should be greater than 3");

// Test assertFalse
assertFalse(false, "false should be false");
assertFalse(1 == 2, "1 should not equal 2");
assertFalse(3 > 5, "3 should not be greater than 5");

// Test assertEqual
assertEqual(5, 5, "5 should equal 5");
assertEqual("hello", "hello", "strings should match");
assertEqual(null, null, "null should equal null");

// Test assertNotEqual
assertNotEqual(5, 10, "5 should not equal 10");
assertNotEqual("hello", "world", "different strings should not match");
assertNotEqual(null, 0, "null should not equal 0");

// Test assertNull and assertNotNull
x = null;
assertNull(x, "x should be null");

y = 42;
assertNotNull(y, "y should not be null");

print("All assertion tests completed!");