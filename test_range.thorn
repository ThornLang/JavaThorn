// Test range function
import { range } from "collections";

// Test basic range
print("range(5): " + range(5));
print("range(10): " + range(10));

// Test with for loop
print("For loop with range(5):");
for (i in range(5)) {
    print("i = " + i);
}

// Test range in computation
sum = 0;
for (i in range(10)) {
    sum = sum + i;
}
print("Sum of range(10): " + sum);

print("Range function tests completed!");