// String operations benchmark
@immut ITERATIONS = 1000;

start_time: number = clock();

result: string = "";
i: number = 0;
while (i < ITERATIONS) {
    result = result + "Hello";
    i = i + 1;
}

end_time: number = clock();

print("Iterations: " + ITERATIONS);
print("Result length: " + result.length);
print("Time: " + (end_time - start_time) + "ms");